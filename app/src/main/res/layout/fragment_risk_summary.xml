                        android:singleLine="true"
                        android:text=""
                        android:id="@+id/util2"
                        android:background="@drawable/capacitygreen"
                        android:textAppearance="?android:attr/textAppearanceLarge"
                        android:textColor="#ffffffff"
                        android:textAlignment="center"
                        android:gravity="center" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text=""
                        android:id="@+id/capacitytxt2"
                        android:layout_below="@+id/util2"
                        android:textStyle="normal"
                        android:textSize="20dp"  android:layout_gravity="center"/>
                </LinearLayout>
                <LinearLayout
                    android:id="@+id/psl1Ctr"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerInParent="true"
                    android:orientation="vertical"
                    android:layout_weight="1.0" >

                    <TextView android:layout_width="wrap_content" android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:singleLine="true"
                        android:text=""
                        android:id="@+id/util3"
                        android:background="@drawable/capacitygreen"
                        android:textAppearance="?android:attr/textAppearanceLarge"
                        android:textColor="#ffffffff"
                        android:textAlignment="center"
                        android:gravity="center"/>

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text=""
                        android:id="@+id/capacitytxt3"
                        android:textStyle="normal"
                        android:textSize="20dp" android:layout_gravity="center" />
                </LinearLayout>
            </LinearLayout>
            <LinearLayout
                android:orientation="horizontal"
                android:id="@+id/riskcapacityrow2"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/riskcapacityrow1"
                android:layout_marginRight="10dp"
                android:layout_marginTop="45dp">

                <LinearLayout
                    android:id="@+id/mixCtr"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_toRightOf="@id/catalystCtr"
                    android:orientation="vertical"
                    android:layout_weight="1.0" >
                    <TextView android:layout_width="wrap_content" android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:singleLine="true"
                        android:text=""
                        android:id="@+id/util4"
                        android:background="@drawable/capacitygreen"
                        android:textAppearance="?android:attr/textAppearanceLarge"
                        android:textColor="#ffffffff"
                        android:textAlignment="center"
                        android:gravity="center" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text=""
                        android:id="@+id/capacitytxt4"
                        android:textStyle="normal"
                        android:textSize="20dp"  android:layout_gravity="center"/>
                </LinearLayout>


                <LinearLayout
                    android:id="@+id/ha1ctr"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerInParent="true"
                    android:orientation="vertical"
                    android:layout_weight="1.0" >

                    <TextView android:layout_width="wrap_content" android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:singleLine="true"
                        android:text=""
                        android:id="@+id/util5"
                        android:background="@drawable/capacitygreen"
                        android:textAppearance="?android:attr/textAppearanceLarge"
                        android:textColor="#ffffffff"
                        android:textAlignment="center"
                        android:gravity="center"/>

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text=""
                        android:id="@+id/capacitytxt5"
                        android:textStyle="normal"
                        android:textSize="20dp" android:layout_gravity="center" />
                </LinearLayout>
            </LinearLayout>
</RelativeLayout>






        <RelativeLayout android:layout_width="match_parent"
            android:layout_height="360dp"
            android:id="@+id/riskworkloadLayout" android:layout_margin="10dp">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="At Risk Pods By Workload"
            android:id="@+id/workload_txt"
            android:textSize="24dp"
            android:textStyle="bold"/>
        <LinearLayout
            android:orientation="horizontal"
            android:id="@+id/riskworkrow1"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/workload_txt"
            android:layout_marginRight="10dp"
            android:layout_marginTop="35dp">
            <LinearLayout
                android:id="@+id/workpslCtr"
                android:layout_width="wrap_content"
