<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    >

    <LinearLayout android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <RelativeLayout android:layout_width="match_parent"
            android:layout_height="360dp"
            android:id="@+id/riskHealthLayout" android:layout_margin="10dp">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="At Risk Pods By Health"
                android:id="@+id/health_txt"
                android:textStyle="bold"
                android:layout_alignParentTop="true"
                android:textSize="24dp" />
            <LinearLayout
                android:orientation="horizontal"
                android:id="@+id/riskhealthrow1"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/alarm_txt"
                android:layout_marginRight="10dp"
                android:layout_marginTop="35dp">


                <LinearLayout
                    android:id="@+id/eastctr"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerInParent="true"
                    android:orientation="vertical"
                    android:layout_weight="1.0" >

                    <TextView android:layout_width="wrap_content" android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:singleLine="true"
                        android:text=""
                        android:id="@+id/health1"
                        android:background="@drawable/healthred"
                        android:textAppearance="?android:attr/textAppearanceLarge"
                        android:textColor="#ffffffff"
                        android:textAlignment="center"
                        android:gravity="center"/>

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text=""
                        android:id="@+id/healthtxt1"
                        android:textStyle="normal"
                        android:textSize="20dp" android:layout_gravity="center" />
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/haCtr"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_toRightOf="@id/eastctr"
                    android:orientation="vertical"
                    android:layout_weight="1.0" >
                    <TextView android:layout_width="wrap_content" android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:singleLine="true"
                        android:text=""
                        android:id="@+id/health2"
                        android:background="@drawable/healthred"
                        android:textAppearance="?android:attr/textAppearanceLarge"
                        android:textColor="#ffffffff"
                        android:textAlignment="center"
                        android:gravity="center" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text=""
                        android:id="@+id/healthtxt2"
                        android:layout_below="@+id/health2"
                        android:textStyle="normal"
                        android:textSize="20dp"  android:layout_gravity="center"/>
                </LinearLayout>


                <LinearLayout
                    android:id="@+id/pslCtr"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerInParent="true"
                    android:orientation="vertical"
                    android:layout_weight="1.0" >

                    <TextView android:layout_width="wrap_content" android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:singleLine="true"
                        android:text=""
                        android:id="@+id/health3"
                        android:background="@drawable/healthred"
                        android:textAppearance="?android:attr/textAppearanceLarge"
                        android:textColor="#ffffffff"
                        android:textAlignment="center"
                        android:gravity="center"/>

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text=""
                        android:id="@+id/healthtxt3"
                        android:textStyle="normal"
                        android:textSize="20dp" android:layout_gravity="center" />
                </LinearLayout>
            </LinearLayout>

            <LinearLayout
                android:orientation="horizontal"
                android:id="@+id/riskhealthrow2"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/riskhealthrow1"
                android:layout_marginRight="10dp"
                android:layout_marginTop="45dp">

                <LinearLayout
                    android:id="@+id/mpCtr"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_toRightOf="@id/eastctr"
                    android:orientation="vertical"
                    android:layout_weight="1.0" >
                    <TextView android:layout_width="wrap_content" android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:singleLine="true"
                        android:text=""
                        android:id="@+id/health4"
                        android:background="@drawable/healthred"
                        android:textAppearance="?android:attr/textAppearanceLarge"
                        android:textColor="#ffffffff"
                        android:textAlignment="center"
                        android:gravity="center" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text=""
                        android:id="@+id/healthtxt4"
                        android:textStyle="normal"
                        android:textSize="20dp"  android:layout_gravity="center"/>
                </LinearLayout>


                <LinearLayout
                    android:id="@+id/ubcCtr"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerInParent="true"
                    android:orientation="vertical"
                    android:layout_weight="1.0" >

                    <TextView android:layout_width="wrap_content" android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:singleLine="true"
                        android:text=""
                        android:id="@+id/health5"
                        android:background="@drawable/healthred"
                        android:textAppearance="?android:attr/textAppearanceLarge"
                        android:textColor="#ffffffff"
                        android:textAlignment="center"
                        android:gravity="center"/>

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text=""
                        android:id="@+id/healthtxt5"
                        android:textStyle="normal"
                        android:textSize="20dp" android:layout_gravity="center" />
                </LinearLayout>
            </LinearLayout>
      </RelativeLayout>

        <RelativeLayout android:layout_width="match_parent"
